<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 자주 사용하는 type의 별칭을 등록해놓고 Mapper.xml에서 사용할 수 있다. -->
	<typeAliases>

		<typeAlias type="com.acorn.lookat.notice.dto.NoticeDto" alias="noticeDto"/>
		<typeAlias type="com.acorn.lookat.users.dto.UsersDto" alias="usersDto"/>
		<typeAlias type="com.acorn.lookat.review.dto.ReviewDto" alias="reviewDto"/>
		<typeAlias type="com.acorn.lookat.booking.dto.BookingDto" alias="bookingDto"/>
		<typeAlias type="com.acorn.lookat.shop.dto.ShopDto" alias="shopDto"/>
		<typeAlias type="com.acorn.lookat.admin.dto.AdminDto" alias="adminDto"/>
	</typeAliases>
	<!-- 필요한 Mapper.xml 등록하기 -->
	<mappers>
		<mapper resource="com/acorn/lookat/mybatis/NoticeMapper.xml"/>
		<mapper resource="com/acorn/lookat/mybatis/UsersMapper.xml"/>
		<mapper resource="com/acorn/lookat/mybatis/ReviewMapper.xml"/>
		<mapper resource="com/acorn/lookat/mybatis/BookingMapper.xml"/>
		<mapper resource="com/acorn/lookat/mybatis/ShopMapper.xml"/>
		<mapper resource="com/acorn/lookat/mybatis/AdminMapper.xml"/>
	</mappers>
</configuration>